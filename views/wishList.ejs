<body>
        <%- include('includes/navigation.ejs') %>

    <main>
        <h1>List of all them sweet Wishes</h1>
        
        <% if(items.length > 0) { %>
        <div class="grid">
            <% for(let item of items) { %>

						<div class="wish">
						
							<p><%= item.title %></p>

						</div>

            <% } %>
        </div> 
        <% } else { %>
            <h1>No Items Found</h1>
        <% } %>
    </main>
<%- include('includes/end.ejs') %>